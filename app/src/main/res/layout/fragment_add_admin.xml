<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.shishir.blood.Fragment.AddAdminFragment">

    <ListView
        android:id="@+id/addAdminListView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:choiceMode="multipleChoice"
        android:layout_above="@+id/addAdminBtn"></ListView>

    <Button
        android:id="@+id/addAdminBtn"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_alignParentBottom="true"
        android:background="@drawable/back_button_two"
        android:text="Add"
        android:textColor="@color/white"
        android:textSize="@dimen/normalTextSize" />
</RelativeLayout>
